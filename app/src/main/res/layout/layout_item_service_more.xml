<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_marginBottom="@dimen/dp_60">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginRight="@dimen/dp_16"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/ll_contact"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_125"
            android:layout_marginTop="@dimen/dp_17"
            android:background="@drawable/yuan_tag_white"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dp_29"
                    android:layout_height="@dimen/dp_24"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_25"
                    android:src="@mipmap/pic8" />

                <ImageView
                    android:layout_width="@dimen/dp_29"
                    android:layout_height="@dimen/dp_24"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_25"
                    android:src="@mipmap/video" />
            </LinearLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_12"
                android:text="联系我们"
                android:textColor="@color/dark"
                android:textSize="@dimen/sp_18" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_8"
                android:text="为您服务，分机转接功能"
                android:textColor="@color/sq_text_gray"
                android:textSize="@dimen/sp_14" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_more_service"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_117"
            android:layout_marginTop="@dimen/dp_17"
            android:background="@drawable/yuan_tag_white"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="@dimen/dp_29"
                    android:layout_height="@dimen/dp_24"
                    android:layout_marginLeft="@dimen/dp_20"
                    android:layout_marginTop="@dimen/dp_25"
                    android:src="@mipmap/pic9" />

                <ImageView
                    android:layout_width="@dimen/dp_29"
                    android:layout_height="@dimen/dp_24"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginTop="@dimen/dp_25"
                    android:src="@mipmap/video" />
            </LinearLayout>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_12"
                android:text="更多服务"
                android:textColor="@color/dark"
                android:textSize="@dimen/sp_18" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_8"
                android:text="生活小帮助"
                android:textColor="@color/sq_text_gray"
                android:textSize="@dimen/sp_14" />


        </LinearLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_30"
            android:gravity="center_horizontal"
            android:text="——— 更多功能，尽情期待 ———"
            android:textColor="@color/sq_text_gray"
            android:textSize="@dimen/sp_16" />

    </LinearLayout>


</LinearLayout>
